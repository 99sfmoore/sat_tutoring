<% provide(:title, 'New Student') %>
<h1> Create new student</h1>
<div class="row">
  <div class="span6 offset3">
    <%= form_for @student, :html => {:multipart => true} do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "Add student", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>



