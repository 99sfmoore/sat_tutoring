<% provide(:title, 'Edit Student') %>
<h1> Update Student Information</h1>
<div class="row">
  <div class="col-md-5 col-md-offset-2">
    <%= form_for @student, :html => {:multipart => true} do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "Save Changes", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>



