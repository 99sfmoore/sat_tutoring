<% provide(:title, 'Edit Site') %>
<h1 class="secondary">Edit site</h1>
<div class="row">
  <div class="col-sm-5 col-sm-offset-1">
    <%= form_for @site, multipart: true do |f| %>
      <%= render 'fields', f: f %>
      
      <%= f.submit "Save Changes", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
